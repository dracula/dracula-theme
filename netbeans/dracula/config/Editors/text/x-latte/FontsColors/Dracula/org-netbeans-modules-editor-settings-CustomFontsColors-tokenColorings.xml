<fontscolors>
    <fontcolor default="whitespace" name="latte-markup-whitespace"/>
    <fontcolor foreColor="ff9e8541" name="latte-delimiter">
        <font style="bold"/>
    </fontcolor>
    <fontcolor foreColor="ff6d3206" name="latte-markup-variable"/>
